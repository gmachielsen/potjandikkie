.css\";\n\nconst AdminDashboard = () => {\n  const [orders, setOrders] = useState([]);\n  const { user } = useSelector((state) => ({ ...state }));\n\n  useEffect(() => {\n    loadOrders();\n  }, []);\n\n  const loadOrders = () =>\n    getOrders(user.token).then((res) => {\n      console.log(JSON.stringify(res.data, null, 4));\n      setOrders(res.data);\n    });\n\n  const handleStatusChange = (orderId, orderStatus) => {\n    changeStatus(orderId, orderStatus, user.token).then((res) => {\n      toast.success(\"Status updated\");\n      loadOrders();\n    });\n  };\n\n  return (\n    <div className=\"container-fluid\">\n              {/* <div className=\"col-md-2\">\n          <AdminNav />\n        </div> */}\n      <div className=\"row\">\n        <div className=\"col-md-2\" style={{ backgroundColor: '#001529' }}>\n          <br></br><br></br>\n          <AdminNav style={{ color: '#FFFFFFA6' }}/>\n        </div>\n\n        <div className=\"col-md-10\">\n          <h4>Admin Dashboard</h4>\n          <Orders orders={orders} handleStatusChange={handleStatusChange} />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default AdminDashboard;\n","/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/functions/auth.js",[],"/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/pages/admin/category/CategoryCreate.js",[],"/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/pages/admin/subject/SubjectCreate.js",[],"/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/pages/admin/category/CategoryUpdate.js",["563"],"/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/pages/admin/sub/SubCreate.js",["564"],"/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/pages/user/Password.js",[],"/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/pages/admin/subject/SubjectUpdate.js",["565"],"/Users/gijsmachielsen/Dokumenten/node/Artisthub/client/src/pages/admin/style/StyleUpdate.js",["566"],"import React, {useEffect, useState} from '